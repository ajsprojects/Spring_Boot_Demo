CREATE TABLE Users(
id INT NOT NULL AUTO_INCREMENT,
name VARCHAR(30) NOT NULL,
email VARCHAR(30) NOT NULL,
postcode VARCHAR(30) NOT NULL,
age INT DEFAULT NULL,
PRIMARY KEY (id));

